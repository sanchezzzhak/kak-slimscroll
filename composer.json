{
    "name": "kak/slimscroll",
    "description": "Yii2 slimscroll widget",
    "type": "yii2-extension",
    "homepage": "https://github.com/sanchezzzhak/kak-panel",
    "keywords": ["yii2","slimscroll ","slimscroll for yii2" , "widget"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tutik Alexsandr",
            "email": "sanchezzzhak@ya.ru"
        }
    ],
    "require": {
	"bower-asset/slimscroll": "*"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
	"psr-4": {
		"kak\\widgets\\slimscroll\\": ""
	}
    }
}
